{
  "scripts": {
    "test": "npx cypress test",
    "open": "npx cypress open",
    "merge-reports": "mochawesome-merge cypress/results/*.json > mochawesome.json",
    "generate-report": "marge mochawesome.json --reportDir reports",
    "report": "npm run merge-reports && npm run generate-report",
    "test:qauto": "cypress run --config-file cypress.config.qauto.js",
    "test:qauto2": "cypress run --config-file cypress.config.qauto2.js"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "cypress": "^15.6.0",
    "marge": "^1.0.1",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.2"
  },
  "dependencies": {
    "cypress-plugin-api": "^2.11.2"
  }
}
